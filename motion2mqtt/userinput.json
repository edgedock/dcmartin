{
  "global": [],
  "services": [
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.motion2mqtt",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "MOTION_TIMEZONE": "America/Los_Angeles",
        "MOTION_GROUP": "newman",
        "MOTION_DEVICE": "",
        "MOTION_POST_PICTURES": "center",
        "MOTION_LOG_LEVEL": 6,
        "MOTION_LOG_TYPE": "all",
        "MQTT_HOST": "mqtt",
        "MQTT_USERNAME": "",
        "MQTT_PASSWORD": "",
        "DEBUG": true
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.mqtt2kafka",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
	"MQTT_HOST": "mqtt",
        "MQTT_USERNAME": "",
        "MQTT_PASSWORD": "",
        "MQTT2KAFKA_APIKEY": "",
        "MQTT2KAFKA_PAYLOAD": "image",
        "MQTT2KAFKA_SUBSCRIBE": "+/+/+/event/end",
        "MQTT2KAFKA_PUBLISH": "",
        "MQTT2KAFKA_TOO_OLD": 300,
        "DEBUG": true
      }
    },
    {
      "org": "${HZN_ORG_ID}",
      "url": "com.github.dcmartin.open-horizon.yolo4motion",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
        "MQTT_HOST": "mqtt",
        "MQTT_USERNAME": "",
        "MQTT_PASSWORD": "",
        "MOTION_GROUP": "newman",
	"YOLO_CONFIG": "tiny",
	"YOLO_ENTITY": "all",
	"YOLO_SCALE": "none",
        "DEBUG": true
      }
    }
  ]
}
